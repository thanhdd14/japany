<main class="page-<%= pageData.template %>">
	<section class="sec-mv">
		<div class="inner">
			<div class="mv-time">
				<div class="mv-time--left">
					<ul class="mv-time__note">
						<li class="lv1">1.0h</li>
						<li class="lv2">2.0h</li>
						<li class="lv3">3.0h</li>
						<li class="lv4">4.0h</li>
						<li class="lv5">5.0h</li>
					</ul>
					<ul class="mv-time__list">
						<li class="mv-time__list-item">
							<div class="pBar" data-to="30"></div>
							<span class="mv-time__list-day"><span>1/2</span>月</span>
						</li>
						<li class="mv-time__list-item">
							<div class="pBar" data-to="77"></div>
							<span class="mv-time__list-day"><span>1/3</span>火</span>
						</li>
						<li class="mv-time__list-item">
							<div class="pBar" data-to="53"></div>
							<span class="mv-time__list-day"><span>1/4</span>水</span>
						</li>
						<li class="mv-time__list-item">
							<div class="pBar" data-to="100"></div>
							<span class="mv-time__list-day"><span>1/5</span>木</span>
						</li>
						<li class="mv-time__list-item">
							<div class="pBar" data-to="10"></div>
							<span class="mv-time__list-day"><span>1/6</span>金</span>
						</li>
						<li class="mv-time__list-item today">
							<div class="pBar" data-to="30"></div>
							<span class="mv-time__list-day"><span>1/7</span>土</span>
						</li>
						<li class="mv-time__list-item">
							<div class="pBar" data-to="0"></div>
							<span class="mv-time__list-day"><span>1/8</span>日</span>
						</li>
					</ul>
					<a href="" class="mv-time__btn mv-time__btn--prev "></a>
					<a href="" class="mv-time__btn mv-time__btn--next disabled"></a>
				</div>
				<div class="mv-time--right">
					<div class="mv-time__oder">
						<div class="mv-time__oder-item">
							<span class="mv-time__oder-ttl">今週の学習時間</span>
							<span class="mv-time__oder-number c-fc--yellow">
								09:00 <small>h</small>
							</span>
						</div>
						<div class="mv-time__oder-item">
							<span class="mv-time__oder-ttl">先週の学習時間</span>
							<span class="mv-time__oder-number c-fc--gray">
								09:00 <small>h</small>
							</span>
						</div>
					</div>
				</div>

			</div>
		</div>
	</section>
